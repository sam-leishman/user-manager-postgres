extends layout

block content
    h2 Give us all of your personal information
    form(method='POST', action='/createUser')
        input(id='first_name', name='first_name', placeholder='first name')
        input(id='last_name', name='last_name', placeholder='last name')
        input(id='email', name='email', type='email', placeholder='email')
        input(id='age', name='age', type='number', placeholder='age')
        input(type='submit')